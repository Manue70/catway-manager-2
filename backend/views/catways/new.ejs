<% layout('layout', { title: 'Nouveau catway' }) %>
<h2>Ajouter un catway</h2>
<form method="post" action="/catways">
  <label>Numéro <input type="number" name="catwayNumber" required></label>
  <label>Type
    <select name="catwayType" required>
      <option value="short">short</option>
      <option value="long">long</option>
    </select>
  </label>
  <label>État <input type="text" name="catwayState"></label>
  <button type="submit">Créer</button>
</form>
